          },
          "date_to": {
            "type": "string",
            "format": "date",
            "description": "End date of the report",
            "example": "2024-04-20"
          },
          "workflows": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "id": {
                  "type": "integer",
                  "description": "The ID of the workflow",
                  "example": 789
                },
                "name": {
                  "type": "string",
                  "description": "The name of the workflow",
                  "example": "Content Creation"
                },
                "status": {
                  "type": "string",
                  "description": "The status of the workflow",
                  "example": "completed"
                }
              }
            }
          }
        }
      },
